{
  "authors": [
    {
      "name": "Wouter De Schuyter",
      "email": "wouter.de.schuyter@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Wouterds\\": "src",
      "Wouterds\\Tests\\": "tests"
    }
  },
  "scripts": {
    "migrations:migrate": "docker exec -i wouterdeschuyter-website-php-fpm ./vendor/bin/doctrine-migrations migrations:migrate",
    "migrations:generate": "docker exec -i wouterdeschuyter-website-php-fpm ./vendor/bin/doctrine-migrations migrations:generate"
  },
  "require": {
    "doctrine/dbal": "^2.6",
    "jenssegers/lean": "^0.2.2",
    "league/container": "^2.4",
    "slim/slim": "^3.8",
    "twig/twig": "^2.4",
    "vlucas/phpdotenv": "^2.4"
  },
  "require-dev": {
    "doctrine/migrations": "^1.5",
    "symfony/var-dumper": "^3.3"
  }
}
